<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wave.backend.mapper.CartItemMapper">

    <resultMap id="BaseResultMap" type="com.wave.backend.entity.CartItem">
        <id property="id" column="id" jdbcType="INTEGER"/>
        <result property="cartId" column="cartId" jdbcType="INTEGER"/>
        <result property="countInCar" column="countInCar" jdbcType="INTEGER"/>
        <result property="price" column="price" jdbcType="DECIMAL"/>
        <result property="bookId" column="bookId" jdbcType="INTEGER"/>
        <result property="cover" column="cover" jdbcType="VARCHAR"/>
        <result property="bookName" column="bookName" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,cartId,countInCar,
        price,bookId,cover,bookName
    </sql>
</mapper>
